{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Build Tracker",
		"version":"1.0.1",
		"minimum-overwolf-version":"0.77.10",
		"author":"Casey Sigelmann",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Keep track of your builds to make it easier to switch between them."
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":1000, "height":700},
					"min_size":{"width":200, "height":200},
					"max_size":{"width":1200, "height":900}
				},
			"SubWindow":{
					"file":"Files/subwindow.html",
					"transparent":true,
					"resizable":false,
					"size":{"width":400, "height":300},
					"in_game_only":true
				}
		}
	}
}